<ProgressbarPopup>:
  # ROLE: Display a popup with progress bar
  # DISPLAY: Title of the progress bar
  #          Progress bar

  auto_dimiss: False
  size_hint: [0.9, 0.2]
  title_align: 'center'
  separator_color: [0, 0, 0, 1]
  
  ProgressBar:
    value: root.progressbarValue
    max: root.progressbarMax  
